<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mysqltest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mysqltest.NotificationBuilder"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- <activity -->
        <!-- android:name="com.example.mysqltest.Register" -->
        <!-- android:label="@string/app_name"></activity> -->
        <activity
            android:name="com.example.mysqltest.Login"
            android:label="Builder" />
        <activity
            android:name="com.example.mysqltest.ReportUpdater"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.mysqltest.ReportViewer"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.mysqltest.OptionFeedbackActivity"
            android:label="Conversation"
            android:parentActivityName="com.example.mysqltest.NotificationBuilder" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NotificationBuilder" />
        </activity>
        <activity
            android:name="com.example.mysqltest.ActionFeedbackActivity"
            android:label="@string/title_activity_action_feedback"
            android:parentActivityName="com.example.mysqltest.NotificationBuilder" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".NotificationBuilder" />
        </activity>
    </application>

</manifest>